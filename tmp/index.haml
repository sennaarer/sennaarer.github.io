!!!
%html(ng-app="sennaarer.github.io" ng-controller="AppCtrl")
  %head
    %meta(charset="utf-8")
    %title(ng-bind="windowTitle") Loading window title
    %link(rel="stylesheet" href="library/css/main.css")
    %link(rel="stylesheet" href="library/css/angular-animate.css")

  %body(ng-controller="topCtrl")
    .container
      .col-md-6
        .second 1
      .col-md-6
        .third 2
      .col-md-6
        .forth 3
      .col-md-6
        .fifth 4

    .container
      .col-lg-2.col-md-2.col-sm-6.col-xs-12
        .first.text-center
          %a(href="/" ) Home
      .col-lg-2.col-md-2.col-sm-6.col-xs-12

      .col-lg-2.col-md-2.col-sm-6.col-xs-12(ng-repeat="part in parts | orderBy: 'title'")
        .text-center(class="{{part.class}}" ng-class="isAct('/'+part.path)" part="'/'+part.path" open="open('/'+part.path)" status="")
          {{part.title}}
    .container.top10(ng-view="")




    .container

      %li(class="dropdown")
        %a(href="#" class="dropdown-toggle")
          %i(class="icon-comment icon-white")
          Discuss
          %b(class="caret")
        %ul(class="dropdown-menu")
          %li
            %a(href="http://blog.angularjs.org") Blog 1
          %li
            %a(href="http://blog.angularjs.org") Blog 2
          %li
            %a(href="http://blog.angularjs.org") Blog 3
          %li
            %a(href="http://blog.angularjs.org") Blog 4
          %li
            %a(href="http://blog.angularjs.org") Blog 5
          %li
            %a(href="http://blog.angularjs.org") Blog 6






  %script(src="library/js/vendor/angular/angular.js" )
  %script(src="library/js/vendor/angular/angular-route.js")
  %script(src="library/js/vendor/angular/angular-animate.js")
  %script(src="library/js/app.js" )
  %script(src="library/js/controllers.js" )
  %script(src="library/js/services.js" )
  %script(src="library/js/directives.js" )
